{
  "canisters": {
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/download/release-2025-07-31/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/download/release-2025-07-31/internet_identity_dev.wasm.gz",
      "init_arg": "(opt record { captcha_config = opt record { max_unsolved_captchas= 50:nat64; captcha_trigger = variant {Static = variant {CaptchaDisabled}}}})",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {}
    },
    "ui": {
      "frontend": {
        "entrypoint": "build/index.html"
      },
      "source": ["build"],
      "gzip": true,
      "optimize": "cycles",
      "type": "assets",
      "dependencies": []
    },
    "user": {
      "main": "src/actor_user/User.mo",
      "type": "motoko",
      "optimize": "cycles",
      "args": "--enhanced-orthogonal-persistence",
      "dependencies": []
    }
  },
  "defaults": {
    "build": {
      "packtool": "mops sources"
    }
  },
  "networks": {
    "staging": {
      "providers": ["https://icp0.io"],
      "type": "persistent"
    }
  },
  "dfx": "0.28.0",
  "version": 1
}
